<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {{ encore_entry_link_tags('app') }}
    <!-- Bootstrap -->
    {{ encore_entry_link_tags('app') }}
    <link href="{{ asset('build/bootstrap5/css/bootstrap.min.css') }}" rel="stylesheet">
    {% block stylesheets %}

    {% endblock %}

    {{ encore_entry_script_tags('app') }}
    <script src="{{ asset('build/bootstrap5/js/bootstrap.bundle.min.js') }}"></script>
    {% block javascripts %}
    {% endblock %}
</head>
<body>

{% if app.request.get('_route') not in ['app_login'] %}
    {% include '_themes/bootstrap5/navbar.html.twig' %}
{% endif %}

{% block content %}

{% endblock %}
</body>
</html>
