<!-- beautify ignore:start -->
<html
        lang="en"
        class="light-style layout-menu-fixed"
        dir="ltr"
        data-theme="theme-default"
        data-assets-path="../assets/"
        data-template="vertical-menu-template-free"
>
<head>
  <meta charset="utf-8" />
  <meta
          name="viewport"
          content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0"
  />

  <title>Bitácora > {% block title %}{% endblock %}</title>

  <meta name="description" content="" />

  <!-- Favicon -->
  <link rel="icon" type="image/x-icon" href="{{ asset('assets/img/favicon_.ico') }}" />

  <!-- Fonts -->
  <link rel="preconnect" href="https://fonts.googleapis.com" />
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
  <link
          href="https://fonts.googleapis.com/css2?family=Public+Sans:ital,wght@0,300;0,400;0,500;0,600;0,700;1,300;1,400;1,500;1,600;1,700&display=swap"
          rel="stylesheet"
  />

  {% block top_styles %}{% endblock %}
  <!-- Icons. Uncomment required icon fonts -->
  <link rel="stylesheet" href="{{ asset('build/sneat/vendor/fonts/boxicons.css') }}" />

  <!-- Core CSS -->
  <link rel="stylesheet" href="{{ asset('build/sneat/vendor/css/core.css') }}" class="template-customizer-core-css" />
  <link rel="stylesheet" href="{{ asset('build/sneat/vendor/css/theme-default.css') }}" class="template-customizer-theme-css" />
  <link rel="stylesheet" href="{{ asset('build/sneat/css/demo.css') }}" />
  <link rel="stylesheet" href="{{ asset('build/sneat/css/custom.css') }}" />
  <link rel="stylesheet" href="{{ asset('build/sneat/css/tagsinput.css') }}" />
  <link rel="stylesheet" href="{{ asset('build/bootstrap5/css/datatables.bootstrap5.min.css') }}" />


  <!-- Vendors CSS -->
  <link rel="stylesheet" href="{{ asset('build/sneat/vendor/libs/perfect-scrollbar/perfect-scrollbar.css') }}" />
  <link rel="stylesheet" href="{{ asset('build/sneat/vendor/libs/apex-charts/apex-charts.css') }}" />

  <!-- Page CSS -->
  {% block bottom_styles %}{% endblock %}
  <!-- Helpers -->
  <script src="{{ asset('build/sneat/vendor/js/helpers.js') }}"></script>

  <!--! Template customizer & Theme config files MUST be included after core stylesheets and helpers.js in the <head> section -->
  <!--? Config:  Mandatory theme config file contain global vars & default theme options, Set your preferred theme option in this file.  -->
  <script src="{{ asset('build/sneat/js/config.js') }}"></script>
</head>

<body>
{{ include('_themes/sneat/spinner.html.twig') }}
{% block body %}
  <!-- Layout wrapper -->
  <div class="layout-wrapper layout-content-navbar">
    <div class="layout-container">
      {% if app.request.get('_route') not in ['app_login'] %}
        {% include '_themes/sneat/sidebar.html.twig' %}
      {% endif %}


      <!-- Layout container -->
      <div class="layout-page">
        <!-- Navbar -->
        {% if app.request.get('_route') not in ['app_login'] %}
          {% include '_themes/sneat/nav.html.twig' %}
        {% endif %}
        <!-- / Navbar -->

        <!-- Content wrapper -->
        <div class="content-wrapper">
          <!-- Content -->

          <div class="container-xxl flex-grow-1 container-p-y">
            {% block content %}

            {% endblock %}
          </div>
          <!-- / Content -->

          <!-- Footer -->
          <footer class="content-footer footer bg-footer-theme">
            <div class="container-xxl d-flex flex-wrap justify-content-between py-2 flex-md-row flex-column">
              <div class="mb-2 mb-md-0">
                ©
                <script>
                  var this_year = new Date().getFullYear();
                  var text = '2021';
                  if(this_year != text){
                    text = text + ' - ' + this_year;
                  }
                  document.write(text);
                </script>
                , desarrollado por <span class="fw-bolder">Yosamy David Bermúdez Pérez</span>
              </div>
            </div>
          </footer>
          <!-- / Footer -->

          <div class="content-backdrop fade"></div>
        </div>
        <!-- Content wrapper -->
      </div>
      <!-- / Layout page -->
    </div>

    <!-- Overlay -->
    <div class="layout-overlay layout-menu-toggle"></div>
  </div>
  {% include '_themes/sneat/flashes.html.twig' %}
{% endblock %}
{% block top_scripts %}{% endblock %}
<script src="{{ asset('build/sneat/vendor/libs/jquery/jquery.js') }}"></script>
<script src="{{ asset('build/sneat/vendor/libs/popper/popper.js') }}"></script>
<script src="{{ asset('build/sneat/vendor/js/bootstrap.js') }}"></script>
<script src="{{ asset('build/sneat/vendor/libs/perfect-scrollbar/perfect-scrollbar.js') }}"></script>
<script src="{{ asset('build/sneat/vendor/js/menu.js') }}"></script>
<script src="{{ asset('build/sneat/vendor/libs/apex-charts/apexcharts.js') }}"></script>
<script src="{{ asset('build/sneat/js/main.js') }}"></script>
<script src="{{ asset('build/sneat/js/dashboards-analytics.js') }}"></script>
<script src="{{ asset('build/sneat/js/ui-toasts.js') }}"></script>
<script src="{{ asset('build/sneat/js/extended-ui-perfect-scrollbar.js') }}"></script>
<script src="{{ asset('build/sneat/js/bootstrap-tagsinput.js') }}"></script>
<script src="{{ asset('build/sneat/js/tagsinput.js') }}"></script>
<script src="{{ asset('build/sneat/js/delete-modal.js') }}"></script>
<script src="{{ asset('build/bootstrap5/js/jquery.dataTables.min.js') }}"></script>
<script src="{{ asset('build/bootstrap5/js/dataTables.bootstrap5.min.js') }}"></script>
<script src="{{ asset('build/bootstrap5/js/dataTables.scroller.min.js') }}"></script>
<script>
  $('.dropdown-menu').each(function () {
    var elem = $(this);
    if(elem.children().length === 0){
      elem.parent().remove();
    }
  });

  var elem = $('#layout-menu a.menu-link[href=\'' + window.location.pathname + '\']');
  if(elem.parent().parent('ul.menu-sub').length){

    elem.parent().parent('ul.menu-sub').closest('li.menu-item').addClass('active open');
  }
  elem.parent().addClass('active');
  //
  $('#myspinner-container').hide();
  $('form').submit(function () {
    $('#myspinner-container').show();
  });
</script>
{% block bottom_scripts %}{% endblock %}
<!-- Place this tag in your head or just before your close body tag. -->
<script async defer src="{{ asset('build/sneat/js/buttons.js') }}"></script>
</body>
</html>
